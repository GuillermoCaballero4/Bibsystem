	<queryString>
		<![CDATA[SELECT 
    d.iddonacion, 
    TO_CHAR(d.fecha_donacion, 'DD-MM-YYYY') AS fecha_donacion, 
    p.nombre_persona, 
    p.ci_persona, 
    d.estado
FROM 
    public.donaciones d
JOIN 
    public.personas p ON d.idpersona = p.idpersona
WHERE
    d.fecha_donacion BETWEEN $P{idfechaa} AND $P{idfechab} AND ($P{estado} = 'Todos' OR d.estado = $P{estado})]]>
	</queryString>
